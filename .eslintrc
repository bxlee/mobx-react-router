{
  "extends": [
    "eslint:recommended", "plugin:react/recommended", "airbnb"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    },
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "rules": {
    "comma-dangle": 0,
    "semi": 2,
    "import/extensions": 0,
    "no-restricted-syntax": 1,
    "no-loop-func": 1,
    "consistent-return": 1,
    "no-nested-ternary": 1,
    "react/require-default-props": 0,
    "react/forbid-prop-types": 0,
    "no-param-reassign": 0,
    "react/prefer-stateless-function": 0,
    "react/no-children-prop": 0,
    "react/jsx-closing-bracket-location": 0,
    "no-plusplus": 0
  }
}
